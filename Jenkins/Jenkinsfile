
pipeline {

    agent any

    tools {
        maven 'Maven 3'  // Specify the Maven version installed in Jenkins' global tools
    }


    stages{
        stage('git checkout'){
            steps{
                git branch: 'main', url: 'https://github.com/Ashikasalian/devops_project.git'

            }
        }

        stage('Build') {
            steps {
                script {
                    sh 'mvn clean install'
                }
            }
        }
}
}
